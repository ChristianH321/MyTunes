      <Button fx:id="btnSaveSong" layoutX="527.0" layoutY="396.0" mnemonicParsing="false" onAction="#savesSong" onMouseClicked="#clickSaveSong" text="Save" textFill="#f80303" />
      <Button fx:id="btnCancelEditSong" layoutX="456.0" layoutY="396.0" mnemonicParsing="false" onAction="#cancelsEditSong" onMouseClicked="#clickCancelEditSong" text="Cancel" textFill="#f20303" />
      <Button fx:id="fileChooser" layoutX="430.0" layoutY="273.0" mnemonicParsing="false" onAction="#choosesSongFiles" onMouseClicked="#clickToOpenWindowToChooseFile" text="Choose..." textFill="#f80303" />
   </children>
</AnchorPane>
